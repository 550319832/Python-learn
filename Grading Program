student_scores = {
    'Harry': 88,
    'Ron': 78,
    'Hermione': 95,
    'Draco': 75,
    'Neville': 60
}

student_grade={}

for student in student_scores:
    
    score=student_scores[student]

    if score>=91:
        student_grade[student]="Outstanding"
    elif score>=81:
        student_grade[student]="Exceed Expectation"
    elif  score>=71:
        student_grade[student]="Acceptable"
    else:
        student_grade[student]="Fail"

print(student_grade)
        

    
